<?xml version="1.0"?>

<PropertyList> 
  <path>engines-gauges.ac</path>


<!-- temp -->
    <animation>
      <name>temp-l</name>
      <type>rotate</type>
      <object-name>needle-temp-l</object-name>
      <property>engines/engine[0]/egt_degf</property>
      <interpolation> 
        <entry><ind>0.0000</ind><dep>118</dep></entry><!--0-->
        <entry><ind>1200</ind><dep>405</dep></entry><!--max thrust-->
      </interpolation>
      <center>
       <x-m>0.0047</x-m>
       <y-m>-0.0781</y-m>
       <z-m>0.000</z-m>
      </center>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    <animation>
      <name>temp-r</name>
      <type>rotate</type>
      <object-name>needle-temp-r</object-name>
      <property>engines/engine[1]/egt_degf</property>
      <interpolation> 
        <entry><ind>0.0000</ind><dep>118</dep></entry><!--0-->
        <entry><ind>1200</ind><dep>405</dep></entry><!--max thrust-->
      </interpolation>
      <center>
       <x-m>0.0047</x-m>
       <y-m>-0.0263</y-m>
       <z-m>0.000</z-m>
      </center>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>

<!-- core rpm -->
    <animation>
      <name>core-rpm-l</name>
      <type>rotate</type>
      <object-name>needle-core-l</object-name>
      <property>engines/engine[0]/n2</property>
      <interpolation>
		<entry><ind>0</ind><dep>0</dep></entry>
		<entry><ind>10</ind><dep>25</dep></entry>
		<entry><ind>20</ind><dep>50</dep></entry>
		<entry><ind>30</ind><dep>75</dep></entry>
		<entry><ind>40</ind><dep>102.2</dep></entry>
		<entry><ind>50</ind><dep>128</dep></entry>
		<entry><ind>60</ind><dep>154</dep></entry>
		<entry><ind>70</ind><dep>184</dep></entry>
		<entry><ind>80</ind><dep>210</dep></entry>
		<entry><ind>90</ind><dep>238</dep></entry>
		<entry><ind>105</ind><dep>272.5</dep></entry>
      </interpolation>
      <center>
       <x-m> 0.00453</x-m>
       <y-m>-0.0788</y-m>
       <z-m> 0.0551</z-m>
      </center>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    <animation>
      <name>core-rpm-r</name>
      <type>rotate</type>
      <object-name>needle-core-r</object-name>
      <property>engines/engine[1]/n2</property>
      <interpolation>
		<entry><ind>0</ind><dep>0</dep></entry>
		<entry><ind>10</ind><dep>25</dep></entry>
		<entry><ind>20</ind><dep>50</dep></entry>
		<entry><ind>30</ind><dep>75</dep></entry>
		<entry><ind>40</ind><dep>102.2</dep></entry>
		<entry><ind>50</ind><dep>128</dep></entry>
		<entry><ind>60</ind><dep>154</dep></entry>
		<entry><ind>70</ind><dep>184</dep></entry>
		<entry><ind>80</ind><dep>210</dep></entry>
		<entry><ind>90</ind><dep>238</dep></entry>
		<entry><ind>105</ind><dep>272.5</dep></entry>
      </interpolation>
      <center>
       <x-m> 0.0047</x-m>
       <y-m>-0.0265</y-m>
       <z-m> 0.0551</z-m>
      </center>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>

